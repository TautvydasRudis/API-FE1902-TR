---
custom_basename: dashboard
---

<div class="container-fluid">
    <div class="row">
        <div class="col-lg-6 col-xl-3">
            <div class="boxes boxes--right">
                <div class="row">
                    <div class="col-4">
                        <div class="circle circle--border circle--success">
                            <svg class="svg-icon">
                                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#visibility"></use>
                            </svg>
                        </div>
                    </div>
                    <div class="col-8 v-align">
                        <h5>64,570</h5>
                        <p>{{dashboard.stats.visits}}</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-6 col-xl-3">
            <div class="boxes boxes--right">
                <div class="row">
                    <div class="col-4">
                        <div class="circle circle--border circle--primary">
                            <svg class="svg-icon">
                                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#timeline"></use>
                            </svg>
                        </div>
                    </div>
                    <div class="col-8 v-align">
                        <h5>64,570</h5>
                        <p>{{dashboard.stats.revenue}}</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-6 col-xl-3">
            <div class="boxes boxes--right">
                <div class="row">
                    <div class="col-4">
                        <div class="circle circle--border circle--danger">
                            <svg class="svg-icon">
                                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#trackChanges"></use>
                            </svg>
                        </div>
                    </div>
                    <div class="col-8 v-align">
                        <h5>64,570</h5>
                        <p>{{dashboard.stats.sales}}</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-6 col-xl-3">
            <div class="boxes boxes--right">
                <div class="row">
                    <div class="col-4">
                        <div class="circle circle--border circle--secondary">
                            <svg class="svg-icon">
                                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#favorite"></use>
                            </svg>
                        </div>
                    </div>
                    <div class="col-8 v-align">
                        <h5>64,570</h5>
                        <p>{{dashboard.stats.conversion}}</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-xl-4">
            <div class="boxes">
                <h6 class="boxes__title">{{dashboard.stats.yearly_sales_report}}</h6>
                <canvas id="myChart1"></canvas>
            </div>
        </div>
        <div class="col-xl-4">
            <div class="boxes">
                <h6 class="boxes__title">{{dashboard.stats.revenue_2}}</h6>
                <canvas id="myChart2"></canvas>
            </div>
        </div>
        <div class="col-xl-4">
            <div class="boxes">
                <h6 class="boxes__title">{{dashboard.stats.revenue_3}}</h6>
                <canvas id="myChart3"></canvas>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-xl-8">
            <div class="boxes">
                <h6 class="boxes__title">{{dashboard.stats.latest_projects}}</h6>
                <p class="boxes__subheading">{{dashboard.stats.awesome_text}}</p>
                <table class="table table-borderless">
                  <thead>
                    <tr>
                      <th scope="col">#</th>
                      <th scope="col">{{dashboard.stats.project_name}}</th>
                      <th scope="col">{{dashboard.stats.start_date}}</th>
                      <th scope="col">{{dashboard.stats.due_date}}</th>
                      <th scope="col">{{dashboard.stats.status}}</th>
                      <th scope="col">{{dashboard.stats.assign}}</th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr>
                      <th scope="row">1</th>
                      <td>Minton Admin v1</td>
                      <td>2016-01-01</td>
                      <td>2017-05-01</td>
                      <td>Released</td>
                      <td>Minton admin</td>
                    </tr>
                    <tr>
                      <th scope="row">2</th>
                      <td>Orbit v1</td>
                      <td>2016-01-01</td>
                      <td>2017-05-01</td>
                      <td>Released</td>
                      <td>Minton admin</td>
                    </tr>
                    <tr>
                      <th scope="row">3</th>
                      <td>Moon v1</td>
                      <td>2016-01-01</td>
                      <td>2017-05-01</td>
                      <td>Released</td>
                      <td>Minton admin</td>
                    </tr>
                    <tr>
                      <th scope="row">4</th>
                      <td>Mars v1</td>
                      <td>2016-01-01</td>
                      <td>2017-05-01</td>
                      <td>Released</td>
                      <td>Minton admin</td>
                    </tr>
                    <tr>
                      <th scope="row">5</th>
                      <td>Venus v1</td>
                      <td>2016-01-01</td>
                      <td>2017-05-01</td>
                      <td>Released</td>
                      <td>Minton admin</td>
                    </tr>
                </tbody>
              </table>
            </div>
        </div>

        <div class="col-xl-4">
            <div class="row">
                <div class="col-12">
                    <div class="boxes boxes--flex">
                        <img class="boxes__image img-fluid" src="../assets/images/alice.jpg" alt="user picture">
                        <div class="boxes__user-info v-align">
                          <h6>Alice</h6>
                          <p>info@aliceeve.com</p>
                          <p class="user-role user-role--admin">Admin</p>
                        </div>
                    </div>
                </div>
                <div class="col-12">
                    <div class="boxes boxes--flex">
                        <img class="boxes__image" src="../assets/images/obama.jpg" alt="user picture">
                        <div class="boxes__user-info v-align">
                          <h6>Obama</h6>
                          <p>barack.obama@usa.gov.com</p>
                          <p class="user-role user-role--senpai">Senpai</p>
                        </div>
                    </div>
                </div>
                <div class="col-12">
                    <div class="boxes boxes--flex">
                        <img class="boxes__image" src="../assets/images/man.jpg" alt="user picture">
                        <div class="boxes__user-info v-align">
                          <h6>John</h6>
                          <p>john.smith@gmail.com</p>
                          <p class="user-role user-role--user">User</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
